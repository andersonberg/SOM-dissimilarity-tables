#include "Mapa.h"

Mapa::Mapa(list<Individual> objetos, int nLinhas, int nColunas, int cardinalidade) {
	list<Cluster> clusters = new list<Cluster>();
	int c = nLinhas * nColunas;
	int i;

	for (i = 0; i < c; i++){
		Cluster cluster = new Cluster(i);
	}


}

