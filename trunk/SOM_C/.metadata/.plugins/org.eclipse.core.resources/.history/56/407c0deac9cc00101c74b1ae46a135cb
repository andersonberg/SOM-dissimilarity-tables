#ifndef CLUSTER_H_
#define CLUSTER_H_

#include <string>
#include <vector>

class Cluster {
public:
	Cluster(int indice);
	virtual ~Cluster();

	void definirPonto(int x, int y);
	void inserirObjeto(Individual objeto);

	int indice;
	std::vector<Individual> prototipos;
	std::vector<Individual> objetos;
	std::vector<float> pesos;
	Point ponto;

};

#endif /* CLUSTER_H_ */
